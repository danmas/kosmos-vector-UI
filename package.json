  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@types/react-syntax-highlighter": "^15.5.13",
    "chromadb": "^3.1.5",
    "d3": "^7.9.0",
    "express": "^4.18.2",
    "faiss-node": "^0.5.1",
    "glob": "^13.0.0",
    "lucide-react": "^0.562.0",
    "minimatch": "^10.1.1",
    "openai": "^6.9.1",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "^3.4.1",
    "tree-sitter": "^0.25.0",
