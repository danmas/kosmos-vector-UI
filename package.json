  "files": [
    "dist/lib",
    "lib",
    "components",
    "services",
    "types.ts",
    "constants.ts"
  ],
  "keywords": [
    "rag",
    "ai",
    "codebase-analysis",
    "react",
    "typescript",
    "knowledge-graph"
