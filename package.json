{
  "name": "@aiitem/ui-components",
  "version": "1.0.0",
  "description": "Reusable UI components for AiItem RAG architecture projects",
  "type": "module",
  "main": "dist/lib/index.js",
  "module": "dist/lib/index.esm.js",
  "types": "dist/lib/types/index.d.ts",
